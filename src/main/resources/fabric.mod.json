    "sylvxa"
  ],
  "contact": {
    "website": "sylvie.lol",
    "repo": "https://github.com/sylvxa/server-side-waystones"
  },
  "license": "MIT",
  "icon": "assets/sswaystones/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "lol.sylvie.sswaystones.Waystones"
    ]
  },
  "mixins": [
    "sswaystones.mixins.json"
  ],
  "accessWidener" : "sswaystones.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ["1.21.4"]
  }
